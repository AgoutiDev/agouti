{
  "name": "libarea/libarea",
  "description": "A platform for collective blogs and social media platform, forum, question and answer service. Website directory, search.",
  "type": "project",
  "keywords": [
    "hleb",
    "libarea",
    "social-network",
    "directory",
    "community",
    "search",
    "facets",
    "blogs",
    "blog",
    "mvc",
    "php"
  ],
  "homepage": "https://github.com/libarea/libarea",
  "license": "MIT",
  "authors": [
    {
      "name": "Evgeny Konchik",
      "email": "dev@libarea.ru",
      "homepage": "https://libarea.ru"
    }
  ],
  "require": {
    "php": ">=7.4.0",
    "ext-pdo": "*",
    "phphleb/framework": ">=1.6.60",
    "phphleb/debugpan": "1.*",
    "phphleb/radjax": "dev-master",
    "donatj/phpuseragentparser": "^1.6",
    "scssphp/scssphp": "^1.10.2",
    "utopia-php/domains": "^1.1",
    "cocur/slugify": "^4.1",
    "phpmailer/phpmailer": "^6.5"
  },
  "autoload": {
    "classmap": [
      "app/",
      "vendor/phphleb",
      "modules/"
    ],
    "psr-4": {
      "App\\": "app/",
      "Phphleb\\": "vendor/phphleb/",
      "Hleb\\": "vendor/phphleb/framework/",
      "Modules\\": "modules/"
    },
    "files": [
        "app/helpers.php"
    ]
  },
  "config": {
    "optimize-autoloader": true
  },
  "minimum-stability": "dev"
}
